{
  "name": "@heroui/styles",
  "version": "3.0.0-beta.6",
  "description": "HeroUI core styles and Tailwind plugin",
  "author": "HeroUI <support@heroui.com>",
  "homepage": "https://heroui.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/heroui-inc/heroui.git",
    "directory": "packages/styles"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "exports": {
    ".": {
      "style": "./dist/index.css",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./css": {
      "style": "./dist/index.css",
      "default": "./dist/index.css"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.js",
      "default": "./dist/components/*/index.js"
    },
    "./base": "./dist/base/base.css",
    "./base/*.css": "./dist/base/*.css",
    "./components/*.css": "./dist/components/*.css",
    "./themes/default": "./dist/themes/default/index.css",
    "./themes/*/*.css": "./dist/themes/*/*.css",
    "./themes/*.css": "./dist/themes/*.css",
    "./utilities": "./dist/utilities/index.css",
    "./variants": "./dist/variants/index.css",
    "./package.json": "./package.json"
  },
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "tailwind-variants": "3.2.2",
    "tw-animate-css": "1.4.0"
  },
  "peerDependencies": {
    "tailwindcss": ">=4.0.0"
  },
  "browser": "./dist/heroui.min.css"
}